{
  "name": "@rogal/smf",
  "version": "1.0.0",
  "description": "Save music to a determined folder",
  "author": "Gabriel Garc√≠a Seco <ggarciaseco@gmail.com>",
  "keywords": [
    "automatization",
    "cli",
    "lazydev"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": "./dist/cli.js",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
    "dev": "ts-node ./src/cli.ts",
    "lint": "front-linter js",
    "clean": "rimraf ./dist/ ./exec/",
    "build": "npm run clean && tsc",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll --coverage",
    "bundle": "npm run build && pkg . --out-dir ./exec/",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@rogal/front-linter": "4.0.0-beta.1",
    "@types/jest": "26.0.22",
    "@types/luxon": "1.26.2",
    "eslint-import-resolver-typescript": "^2.4.0",
    "husky": "4.2.3",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "pkg": "4.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.4",
    "ts-node": "9.1.1",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "@inquirer/input": "0.0.14-alpha.0",
    "@inquirer/password": "0.0.13-alpha.0",
    "commander": "7.2.0",
    "luxon": "1.26.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint -- --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@rogal/front-linter/eslint.js"
    ]
  }
}
